{
  "brand": {
    "name": "Acme Plumbing",
    "logo": "assets/acme.svg",
    "favicon": "assets/acme.png",
    "theme": "styles/themes/acme.css"
  },
  "defaults": {
    "source": "microsoft",
    "campaignType": "organic",
    "trackingMode": "channel"
  },
  "copy": {
    "headline": "Fast, Reliable Repairs",
    "ctaPrimary": "Schedule Service"
  },
  "pages": {
    "serp": "pages/serp.html",
    "landing": "pages/landing.html",
    "checking": "pages/checking.html"
  },
  "phone": {
    "visibleSelectors": [
      "[data-dni]",
      ".dni-number"
    ],
    "format": "(###) ###-####"
  },
  "dniRules": [
    {
      "when": {
        "source": "bing",
        "medium": "organic"
      },
      "number": "+1 (555) 777-2002"
    },
    {
      "when": {
        "source": "direct"
      },
      "number": "+1 (555) 777-9000"
    }
  ],
  "attribution": {
    "utm_campaign": "",
    "utm_term": "water heater repair",
    "landing_page_url": "acme.com/repair/"
  },
  "features": {
    "fbclidOnOrganic": false
  }
}