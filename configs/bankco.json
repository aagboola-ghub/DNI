{
  "meta": {
    "id": "bankco",
    "label": "BankCo \u2014 SMB Banking",
    "version": "1.0.0"
  },
  "branding": {
    "logo": "assets/logos/wf.svg",
    "favicon": "assets/icons/wf.ico"
  },
  "palette": {
    "primary": "#0f766e",
    "secondary": "#ecfeff",
    "accent": "#38bdf8",
    "text": "#0b2530",
    "muted": "#6b7280"
  },
  "copy": {
    "heroHeadline": "Smart banking for growing businesses",
    "ctaPrimary": "Open an account"
  },
  "phone": {
    "pool": [
      "(800) 900-1100",
      "(800) 900-2200"
    ],
    "formatMask": "(###) ###-####"
  },
  "dniRules": [
    {
      "when": {
        "source": "google",
        "campaignType": "paid"
      },
      "number": "(800) 900-1100"
    },
    {
      "when": {
        "source": "google",
        "campaignType": "organic"
      },
      "number": "(800) 900-2200"
    },
    {
      "when": {
        "source": {
          "$in": [
            "facebook",
            "microsoft"
          ]
        }
      },
      "number": "(800) 900-2200"
    }
  ],
  "presets": {
    "trafficSources": [
      "google",
      "facebook",
      "microsoft"
    ],
    "trackingModes": [
      "session",
      "channel"
    ],
    "default": {
      "source": "google",
      "campaignType": "paid",
      "trackingMode": "session"
    }
  },
  "features": {
    "fbclidOnOrganic": false
  }
}