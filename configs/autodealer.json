{
  "meta": {
    "id": "autodealer",
    "label": "AutoDealer \u2014 Sales Appointments",
    "version": "1.0.0"
  },
  "branding": {
    "logo": "assets/logos/wf.svg",
    "favicon": "assets/icons/wf.ico"
  },
  "palette": {
    "primary": "#334155",
    "secondary": "#f1f5f9",
    "accent": "#f97316",
    "text": "#0f172a",
    "muted": "#64748b"
  },
  "copy": {
    "heroHeadline": "Book a test drive today",
    "ctaPrimary": "Call a sales rep"
  },
  "phone": {
    "pool": [
      "(888) 123-4000",
      "(888) 123-5000"
    ],
    "formatMask": "(###) ###-####"
  },
  "dniRules": [
    {
      "when": {
        "source": "google",
        "campaignType": "paid"
      },
      "number": "(888) 123-4000"
    },
    {
      "when": {
        "source": "google",
        "campaignType": "organic"
      },
      "number": "(888) 123-5000"
    },
    {
      "when": {
        "source": {
          "$in": [
            "facebook",
            "microsoft"
          ]
        }
      },
      "number": "(888) 123-5000"
    }
  ],
  "presets": {
    "trafficSources": [
      "google",
      "facebook",
      "microsoft"
    ],
    "trackingModes": [
      "session",
      "channel"
    ],
    "default": {
      "source": "google",
      "campaignType": "paid",
      "trackingMode": "session"
    }
  }
}