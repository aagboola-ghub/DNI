{
  "id": "acme-bank",
  "label": "ACME Bank",
  "brand": {
    "logo": "./assets/logos/acme.svg",
    "tokens": {
      "--brand-bg": "#f8fafc",
      "--brand-fg": "#0b1220",
      "--brand-primary": "#0f766e",
      "--brand-secondary": "#a7f3d0",
      "--brand-accent": "#115e59"
    }
  },
  "numbers": {
    "default": "+1 (888) 555-2200",
    "paid_search": "+1 (888) 555-2201",
    "organic_search": "+1 (888) 555-2202",
    "facebook": "+1 (888) 555-2203",
    "microsoft": "+1 (888) 555-2204"
  },
  "dniRules": [
    {
      "id": "paid-google",
      "when": {
        "source": "google",
        "campaignType": "paid"
      },
      "number": "paid_search"
    },
    {
      "id": "organic-google",
      "when": {
        "source": "google",
        "campaignType": "organic"
      },
      "number": "organic_search"
    },
    {
      "id": "facebook-any",
      "when": {
        "source": "facebook"
      },
      "number": "facebook"
    },
    {
      "id": "microsoft-any",
      "when": {
        "source": "microsoft"
      },
      "number": "microsoft"
    },
    {
      "id": "fallback",
      "when": {},
      "number": "default"
    }
  ],
  "journey": {
    "defaultSource": "google",
    "defaultCampaignType": "paid",
    "trackingMode": "session",
    "pages": [
      "home",
      "products",
      "product-detail",
      "contact"
    ]
  }
}